assert = {
    [true]: nil
    [false]: error['An assertion failed']
    [true, @message]: nil
    [false, @message]: error['An assertion failed: ' + @message]
}

assert== = {
    [@value, @value]: nil
    [@value, @other]: assert[false, 'Expected ' + @value.inspect + ' but got ' + @other.inspect]
}

